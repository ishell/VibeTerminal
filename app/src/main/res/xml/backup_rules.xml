<?xml version="1.0" encoding="utf-8"?>
<!--
    Backup rules for Android 11 and below (API 30-)
    Security: Exclude all data from backup
    This app stores SSH credentials which must never be backed up
-->
<full-backup-content>
    <!-- Exclude everything - SSH credentials must never be backed up -->
    <exclude domain="database" path="." />
    <exclude domain="sharedpref" path="." />
    <exclude domain="file" path="." />
    <exclude domain="external" path="." />
    <exclude domain="root" path="." />
</full-backup-content>
